{
  "name": "@enact/jsdoc-to-ts",
  "version": "1.0.4",
  "description": "Converts inline jsdoc comments to TypeScript .d.ts files",
  "main": "index.js",
  "author": "Roy Sutton <roy.sutton@lge.com>",
  "repository": "https://github.com/enactjs/jsdoc-to-ts",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16"
  },
  "type": "module",
  "bin": {
    "jsdoc-to-ts": "./bin/jsdoc-to-ts.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "enact lint --strict . -- --env=node",
    "test": "mocha --reporter spec"
  },
  "keywords": [
    "jsdoc",
    "typescript"
  ],
  "eslintConfig": {
    "extends": "enact-proxy/strict",
    "env": {
      "node": true
    }
  },
  "dependencies": {
    "documentation": "^14.0.2",
    "glob": "^10.3.10",
    "jsonata": "^2.0.3",
    "loglevel": "^1.8.1",
    "minimist": "^1.2.8",
    "prettier": "^3.2.4"
  },
  "devDependencies": {
    "eslint-config-enact-proxy": "^1.0.7",
    "expect": "^29.7.0",
    "mocha": "^10.2.0",
    "react": "^18.2.0"
  }
}