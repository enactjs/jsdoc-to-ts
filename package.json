{
  "name": "@enact/jsdoc-to-ts",
  "version": "0.1.1",
  "description": "Converts inline jsdoc comments to TypeScript .d.ts files",
  "main": "index.js",
  "author": "Roy Sutton <roy.sutton@lge.com>",
  "repository": "https://github.com/enactjs/jsdoc-to-ts",
  "license": "Apache-2.0",
  "bin": {
    "jsdoc-to-ts": "./bin/jsdoc-to-ts.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "enact lint --strict . -- --env=node",
    "test": "mocha --reporter spec"
  },
  "keywords": [
    "jsdoc",
    "typescript"
  ],
  "eslintConfig": {
    "extends": "enact-proxy/strict",
    "env": {
      "node": true
    }
  },
  "dependencies": {
    "documentation": "^13.2.5",
    "glob": "^7.2.0",
    "jsonata": "^1.8.5",
    "loglevel": "^1.8.0",
    "minimist": "^1.2.5",
    "prettier": "^2.5.1"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "eslint-config-enact-proxy": "^1.0.2",
    "mocha": "^9.1.3",
    "react": "^17.0.2"
  }
}
